{
  "name": "nx-go-playground",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "PORT=3334 pnpx nx  run be-api-service:serve & pnpx nx run be-api-service:serve & pnpx nx run fe-solid-app:serve",
    "start": "nx serves",
    "build": "yarn nx run-many --target=build --parallel --max-parallel=4 --prod --all",
    "lint": "yarn nx run-many --target=lint --parallel --max-parallel=4 --all",
    "serve": "yarn nx run-many --target=serve --parallel --max-parallel=4 --all",
    "test": "yarn nx run-many --target=test --parallel --max-parallel=4 --all",
    "e2e": "yarn nx run-many --target=e2e --parallel --max-parallel=4 --all",
    "ups": "yarn nx run-many --target=up --parallel --max-parallel=4 --all",
    "docker:affected": "yarn nx affected --target=docker --parallel --max-parallel=3",
    "docker": "yarn nx run-many --target=docker --parallel --max-parallel=4 --all",
    "deploy": "yarn nx run-many --target=deploy --parallel --max-parallel=4 --all",
    "affected": "yarn nx affected --parallel=5",
    "version": "yarn nx run-many --target=version --parallel --max-parallel=1 --all -releaseAs=prerelease --preid=beta --push",
    "goreleaser": "yarn nx run-many --target=goreleaser --parallel --max-parallel=1 --all",
    "prepare": "husky install",
    "commit": "cz"
  },
  "private": true,
