{
  "root": "apps/test/be-test-app",
  "sourceRoot": "apps/test/be-test-app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/test/be-test-app",
        "main": "apps/test/be-test-app/src/main.ts",
        "tsConfig": "apps/test/be-test-app/tsconfig.app.json",
        "assets": ["apps/test/be-test-app/src/assets"],
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/test/be-test-app/src/environments/environment.ts",
              "with": "apps/test/be-test-app/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "test-be-test-app:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/test/be-test-app/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/test/be-test-app"],
      "options": {
        "jestConfig": "apps/test/be-test-app/jest.config.js",
        "passWithNoTests": true
      }
    },
    "docker": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "docker buildx build --target nest-builder --platform linux/amd64,linux/arm64 --build-arg DIST_PATH=dist/apps/test/be-test-app . -t yurikrupnik/be-test-app -t europe-west1-docker.pkg.dev/maya-mussia/nx-go-playground/be-test-app --push"
      }
    },
    "packer": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "docker buildx build --target nest-builder --build-arg DIST_PATH=dist/apps/test/be-test-app --platform linux/amd64,linux/arm64 . -t yurikrupnik/be-test-app -t europe-west1-docker.pkg.dev/maya-mussia/nx-go-playground/be-test-app --push"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    }
  },
  "tags": []
}
