{
  "root": "apps/profile/api",
  "sourceRoot": "apps/profile/api/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/profile/api",
        "main": "apps/profile/api/src/main.ts",
        "tsConfig": "apps/profile/api/tsconfig.app.json",
        "assets": [
          "apps/profile/api/src/assets"
        ],
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/profile/api/src/environments/environment.ts",
              "with": "apps/profile/api/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "profile-api:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/profile/api/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/profile/api"
      ],
      "options": {
        "jestConfig": "apps/profile/api/jest.config.js",
        "passWithNoTests": true
      }
    },
    "docker": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "docker buildx build --target nest-builder --build-arg DIST_PATH=dist/apps/profile/api --platform linux/amd64,linux/arm64 . -t yurikrupnik/profile-api -t europe-west1-docker.pkg.dev/nx-go-playground/nx-go-playground/profile-api --push"
      }
    },
    "pack": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "pack build yurikrupnik/buildpack-api --path dist/apps/profile/api/ --builder cnbs/sample-builder:bionic"
      }
    },
    "skaffold": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "skaffold build"
      }
    },
    "docker-alt": {
      "executor": "@nx-tools/nx-docker:build",
      "options": {
        "push": false,
        "target": "nest-builder",
        "context": "./",
        "build-args": [
          "DIST_PATH=dist/apps/profile/api"
        ],
        "tags": [
          "yurikrupnik/profile-api:latest",
          "yurikrupnik/profile-api:v1"
        ]
      }
    }
  },
  "tags": []
}
