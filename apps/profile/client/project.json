{
  "root": "apps/profile/client",
  "sourceRoot": "apps/profile/client/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "swc",
        "outputPath": "dist/apps/profile/client",
        "index": "apps/profile/client/src/index.html",
        "baseHref": "/",
        "main": "apps/profile/client/src/main.tsx",
        "polyfills": "apps/profile/client/src/polyfills.ts",
        "tsConfig": "apps/profile/client/tsconfig.app.json",
        "assets": [
          "apps/profile/client/src/favicon.ico",
          "apps/profile/client/src/assets"
        ],
        "styles": [
          "apps/profile/client/src/styles.css"
        ],
        "scripts": [],
        "webpackConfig": "apps/profile/client/webpack.config.js"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/profile/client/src/environments/environment.ts",
              "with": "apps/profile/client/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "profile-client:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "profile-client:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/profile/client/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "tests": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/profile/client"
      ],
      "options": {
        "jestConfig": "apps/profile/client/jest.config.js",
        "passWithNoTests": true
      }
    },
    "docker": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "docker buildx build --target web-builder --platform linux/amd64,linux/arm64 --build-arg DIST_PATH=dist/apps/profile/client . -t yurikrupnik/profile-client -t europe-west1-docker.pkg.dev/maya-mussia/nx-go-playground/profile-client --push"
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx run profile-client-infrastructure:up"
        ]
      }
    }
  },
  "tags": []
}
