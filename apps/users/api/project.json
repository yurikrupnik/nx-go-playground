{
  "root": "apps/users/api",
  "projectType": "application",
  "sourceRoot": "apps/users/api",
  "targets": {
    "build": {
      "executor": "@nx-go/nx-go:build",
      "options": {
        "outputPath": "dist/apps/users/api",
        "main": "apps/users/api/main.go"
      }
    },
    "serve": {
      "executor": "@nx-go/nx-go:serve",
      "options": {
        "cmd": "gow",
        "cwd": ".",
        "main": "apps/users/api/main.go"
      }
    },
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    },
    "docker": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "commands": [
          "docker build --target go-builder --build-arg DIST_PATH=dist/apps/users/api . -t yurikrupnik/users-api -t europe-west1-docker.pkg.dev/maya-mussia/nx-go-playground/users-api",
          "docker push europe-west1-docker.pkg.dev/maya-mussia/nx-go-playground/users-api",
          "docker push yurikrupnik/users-api"
        ]
      }
    }
  },
  "tags": []
}
