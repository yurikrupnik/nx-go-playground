{
  "root": "apps/users/api",
  "projectType": "application",
  "sourceRoot": "apps/users/api",
  "targets": {
    "build": {
      "executor": "@nx-go/nx-go:build",
      "options": {
        "outputPath": "dist/apps/users/api",
        "main": "apps/users/api/main.go"
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "cwd": "apps/users/api",
        "command": "npx nodemon --exec go run main.go"
      }
    },
    "serves": {
      "executor": "@nx-go/nx-go:serve",
      "options": {
        "main": "apps/users/api/main.go"
      }
    },
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    },
    "docker": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "docker buildx build --target go-builder --build-arg DIST_PATH=dist/apps/users/api . -t yurikrupnik/go-api -t europe-west1-docker.pkg.dev/maya-mussia/nx-go-playground/go-api --push"
      }
    }
  },
  "tags": []
}
