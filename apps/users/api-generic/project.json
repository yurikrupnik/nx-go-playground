{
  "root": "apps/users/api-generic",
  "projectType": "application",
  "sourceRoot": "apps/users/api-generic",
  "targets": {
    "build": {
      "executor": "@nx-go/nx-go:build",
      "options": {
        "outputPath": "dist/apps/users/api-generic",
        "main": "apps/users/api-generic/main.go"
      }
    },
    "serve": {
      "executor": "@nx-go/nx-go:serve",
      "options": {
        "main": "apps/users/api-generic/main.go"
      }
    },
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    },
    "docker": {
      "executor": "@nx-tools/nx-docker:build",
      "options": {
        "push": true,
        "file": "./Dockerfile",
        "target": "go-builder",
        "build-args": ["DIST_PATH=dist/apps/users/api-generic"],
        "metadata": {
          "images": ["yurikrupnik/users-api-generic", "europe-west1-docker.pkg.dev/maya-mussia/nx-go-playground/users-api-generic"]
        }
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    }
  },
  "tags": []
}
